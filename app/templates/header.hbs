   <div class="container-fluid bg-1 text-center">
            <div class="col-md-12">
                <h3>E-VOTING</h3>
                <img src="assets/Image/govt.jpg" class="img-circle" alt="Bird" width="200" height="200">
            </div> 
              
              
              

    <div class="col-md-12">
        <div class ="col-md-6">
                <div class="panel panel-default">
                        <div class="panel-heading">PARTY A</div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-4">
                                Voter ID:
                            </div>
                                <div class="col-md-4">
                                        {{input type="text" class="form-control" value=vote id="vote"}}                                      
                                </div>                                
                            </div>
                            <div class="row">
                                    <div class="col-md-4">
                                            Name:
                                </div>
                                    <div class="col-md-4">
                                        {{input type="text" class="form-control" value=Name name="name" }}
                                    </div>                                
                                </div>
            
                                <div class="row">
                                        <div class="col-md-4">
                                                Age:
                                    </div>
                                        <div class="col-md-4">
                                                {{input type="number"class="form-control"  value=age }}
                                        </div>                                
                                    </div>
                                    <div class="row">
                                            <div class="col-md-4">
                                                    Location:
                                        </div>
                                            <div class="col-md-4">
                                                    {{input type="text" class="form-control" value=city }}
                                            </div>                                
                                        </div>
                                  
                        
                        
                            <!-- <div class="col-md-12 icowo"> -->
                                <img src="assets/Image/vote.jpg"class="img-circle" width="100" height="100"> 
                                <!-- <i class="fa fa-female" aria-hidden="true"></i> -->
                               <div>
                                <button class="btn" {{action "Submit"}}>VOTE</button>
                            </div>
                            </div>
                          <!-- </div> -->
                    </div>
        </div>
        <div class ="col-md-6">
            <div class="panel panel-default">
                <div class="panel-heading">PARTY B</div>
                <div class="panel-body">
                        <div class="row">
                                <div class="col-md-4">
                                Voter ID:
                            </div>
                                <div class="col-md-4">
                                        {{input type="text" class="form-control" value=Vote}}                                       
                                </div>                                
                            </div>
                            <div class="row">
                                    <div class="col-md-4">
                                            Name:
                                </div>
                                    <div class="col-md-4">
                                            {{input type="text" class="form-control" value=name}}
                                    </div>                                
                                </div>
            
                                <div class="row">
                                        <div class="col-md-4">
                                                Age:
                                    </div>
                                        <div class="col-md-4">
                                                {{input type="number"class="form-control" value=number}}
                                        </div>                                
                                    </div>
                                    <div class="row">
                                            <div class="col-md-4">
                                                    Location:
                                        </div>
                                            <div class="col-md-4">
                                                    {{input type="text" class="form-control" value=City}}
                                            </div>                                
                                        </div>
                    <!-- <div class="col-md-12 icowo"> -->
                            <img src="assets/Image/vote.jpg"class="img-circle" width="100" height="100"> 
                        <!-- <i class="fa fa-female" aria-hidden="true"></i> -->
                        <div>
                        <button class="btn"{{action "vote"}} >VOTE</button>
                    </div>
                    </div>
                  <!-- </div> -->
            </div>
        </div>
    </div>
    <div>
        <button class="btn"{{action "results"}}>GET RESULTS</button>
    </div>
</div>

